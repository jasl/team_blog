<h1>Listing links</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th></th>
    <th></th>
  </tr>

<% @links.each do |link| %>
  <tr>
    <td><%= link.name %></td>
    <td><%= link.url %></td>
    <td><%= link_to 'Edit', edit_admin_link_path(link) %></td>
    <td><%= link_to 'Destroy', [:admin, link], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
